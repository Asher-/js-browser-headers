{
  "compilerOptions": {
    "moduleResolution": "node",
    "alwaysStrict": true,
    "sourceMap": true,
    "declaration": true,
    "declarationDir": "./dist/typings",
    "removeComments": true,
    "noImplicitReturns": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictNullChecks": true,
    "stripInternal": true,
    "noFallthroughCasesInSwitch": true,
    "noEmitOnError": true
  },
  "types": [],
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}
